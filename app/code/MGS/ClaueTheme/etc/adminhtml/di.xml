<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <type name="Magento\Cms\Model\Wysiwyg\Images\Storage">
        <arguments>
            <argument name="extensions" xsi:type="array">
                <item name="allowed" xsi:type="array">
                    <item name="glb" xsi:type="number">1</item>
					<item name="jpg" xsi:type="number">1</item>
					<item name="jpeg" xsi:type="number">1</item>
					<item name="png" xsi:type="number">1</item>
					<item name="gif" xsi:type="number">1</item>
					<item name="xml" xsi:type="number">1</item>
                </item>
            </argument>
        </arguments>
    </type>
    <preference for="Magento\Cms\Model\Wysiwyg\Images\Storage" type="MGS\ClaueTheme\Model\Cms\Wysiwyg\Images\Storage" />
    <preference for="MGS\ThemeSettings\Block\Adminhtml\System\ImportStatic" type="MGS\ClaueTheme\Block\Adminhtml\System\ImportStatic" />
    <preference for="MGS\Fbuilder\Block\Adminhtml\System\Import" type="MGS\ClaueTheme\Block\Adminhtml\System\Import" />
</config>
