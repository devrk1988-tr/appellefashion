<script type="text/javascript">
	function toggleEl(el, elId){
		require([
			'jquery',
		], function(jQuery){
			(function($) {
				$('#'+elId).toggleClass('active').slideToggle();
				$(el).toggleClass('active');
                $('#'+elId).parent().toggleClass('is-active');
			})(jQuery);
		});
	}
</script>
